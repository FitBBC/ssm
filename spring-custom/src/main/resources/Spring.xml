<beans>
    <bean id="student" class="com.bbc.spring.po.Student" init-method="">
        <property name="name" value="bbc"></property>
        <property name="course" ref="course"></property>
    </bean>

    <bean id="course" class="com.bbc.spring.po.Course" init-method="init">
        <property name="name" value="java"></property>
        <property name="age" value="18"></property>
    </bean>
</beans>